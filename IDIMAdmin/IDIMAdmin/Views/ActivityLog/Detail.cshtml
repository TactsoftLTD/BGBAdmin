            <h2 class="title pull-left">Detail</h2>
        </header>

        <div class="content-body">
            <div class="row">
                <div class="col-xs-12">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                @Html.LabelFor(m => m.UserId, new { @class = "col-md-2 form-label" })
                                <div class="controls col-md-10">
                                    : @Html.DisplayFor(m => m.UserId)
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                @Html.LabelFor(m => m.UserName, new { @class = "col-md-2 form-label" })
                                <div class="controls col-md-10">
                                    : @Html.DisplayFor(m => m.UserName)
                                </div>
